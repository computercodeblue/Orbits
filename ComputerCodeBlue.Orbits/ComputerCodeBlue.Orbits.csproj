<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Christopher Burke</Authors>
    <Company>Computer Code Blue LLC</Company>
    <Description>A library for working with MyLaps Orbits.</Description>
    <Copyright>2021 Computer Code Blue LLC</Copyright>
    <RepositoryUrl>https://github.com/computercodeblue/Orbits</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/computercodeblue/Orbits</PackageProjectUrl>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="26.1.0" />
  </ItemGroup>

</Project>
